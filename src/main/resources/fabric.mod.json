  "name": "${mod_name}",
  "description": "Control Your Elytra",
  "authors": [
    "Smootheez"
  ],
  "contact": {
    "homepage": "https://github.com/Smootheez/Elytra-Control",
    "sources": "https://github.com/Smootheez/Elytra-Control",
    "issues": "https://github.com/Smootheez/Elytra-Control/issues"
  },
  "license": "GNU General Public License v3.0",
  "icon": "assets/elytracontrol/icon.png",
  "environment": "client",
  "entrypoints": {
    "client": [
      "dev.smootheez.elytracontrol.ElytraControl"
    ],
    "modmenu" : [
      "dev.smootheez.elytracontrol.config.ModMenuApiImpl"
    ]
  },
  "mixins": [
    "elytracontrol.mixins.json"
